<Window x:Class="GeeB_EntityFramework.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel>
        <Button x:Name="addDawgButton"
                Margin="20"
                Click="addDawgButton_Click"
                Content="Add Dawg"></Button>
        <Button x:Name="removeDawgButton"
                Margin="20"
                Click="removeDawgButton_Click"
                Content="Remove Dawg"></Button>
        <DataGrid x:Name="dawgdatagrid"/>
    </StackPanel>
</Window>
